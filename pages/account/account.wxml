<view class="">
  <view class="userinfo">
    <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo" bindgetuserinfo="getUserInfo"> 获取头像昵称 </button>
    <block wx:else>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
  <navigator url="../personalInfo/personalInfo" class="weui-cell weui-cell_access">
    <view class="weui-cell__bd">个人信息</view>
  </navigator>
  <navigator url="../personalInfo/personalInfo" class="weui-cell weui-cell_access">
    <view class="weui-cell__bd">我的收藏</view>
  </navigator>
  <navigator url="../feedback/feedback" class="weui-cell weui-cell_access">
    <view class="weui-cell__bd">意见反馈</view>
  </navigator>
</view>
